{
    "name":        "bmatzner/backbone-bundle",
    "type":        "symfony-bundle",
    "description": "Backbone.js Bundle for Symfony2",
    "keywords":    ["backbone", "javascript", "symfony"],
    "homepage":    "http://backbonejs.org/",
    "license":     "MIT",
    "authors":     [
        {
            "name":     "Bernd Matzner",
            "email":    "bernd@matznermatzner.de",
            "homepage": "http://berndmatzner.de"
        }
    ],
    "require":     {
        "symfony/framework-bundle": ">=2.1",
        "bmatzner/underscore-bundle": "*"
    },
    "recommend":   {
        "bmatzner/jquery-bundle": "*",
        "bmatzner/json2-bundle":  "*",
        "bmatzner/lodash-bundle": "*"
    },
    "autoload":    {
        "psr-0": {
            "Bmatzner\\BackboneBundle": ""
        }
    },
    "target-dir":  "Bmatzner/BackboneBundle"
}
